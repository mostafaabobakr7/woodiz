{% set menu = '' %}
{# TO REMOVE Right click contextmenu set menu='return
false;' #}
<!DOCTYPE html>
<html class="{{ pageClassName }}" data-theme='light' data-themes='theme05' lang='en-US' dir='ltr'>
<meta charset="UTF-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<link rel="apple-touch-icon" sizes="180x180" href="./assets/icons/apple-touch-icon.png">
<link rel="icon" type="image/png" sizes="32x32" href="./assets/icons/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="192x192" href="./assets/icons/android-chrome-192x192.png">
<link rel="icon" type="image/png" sizes="16x16" href="./assets/icons/favicon-16x16.png">
<link rel="manifest" href="./assets/icons/site.webmanifest">
<link rel="mask-icon" href="./assets/icons/safari-pinned-tab.svg" color="#5bbad5">
<link rel="shortcut icon" href="./assets/icons/favicon.ico">
<meta name="msapplication-TileColor" content="#ffffff">
<meta name="msapplication-TileImage" content="./assets/icons/mstile-144x144.png">
<meta name="msapplication-config" content="./assets/icons/browserconfig.xml">
<meta name="theme-color" content="#ffffff">
<!--  -->
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito:400,700,900&display=swap">
<title>{{ title }} - WOODIZ</title>

<body oncontextmenu="{{ menu }}">
  {% block loader %}{% endblock %}
  {% block nav %}{% endblock %}
  {% block body %}{% endblock %}
  {% block footer %}
  {% endblock %}
  {% block script %}{% endblock %}
  <link href='./assets/app.css' rel='stylesheet' type='text/css' />
  <script src='./assets/app.js'></script>
</body>

</html>
